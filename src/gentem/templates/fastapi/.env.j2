# {{ project_name }} Environment Variables

# Application
APP_NAME={{ project_name }}
DEBUG=true
API_V1_STR=/api/v1

# Database
{% if has_database %}
{% if db_type == "sqlite" %}
DATABASE_URL=sqlite+aiosqlite:///./{{ project_slug }}.db
{% else %}
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/{{ project_slug }}
{% endif %}
{% else %}
DATABASE_URL=
{% endif %}
